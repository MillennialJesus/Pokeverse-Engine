if (!state.mtBattle) {
  state.mtBattle = {
    currentZone: 1,             // 1â€“10 (each zone is 10 trainers)
    currentTrainer: 1,          // 1â€“10 within current zone
    maxZoneReached: 1,
    totalTrainersDefeated: 0,
    completed: false
  };
}

