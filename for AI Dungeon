const modifier = (text) => {
  const hiddenNatures = [
    "Empathic", "Vengeful", "Curious", "Proud",
    "Submissive", "Protective", "Wild"
  ];

  // --- Helper: Capitalize for clean display ---
  function capitalize(s) {
    return s ? s.charAt(0).toUpperCase() + s.slice(1).toLowerCase() : "";
  }

  // === UNIVERSAL INIT OF ROOT OBJECTS ===
  if (typeof output === "undefined") output = [];
  if (typeof state === "undefined") state = {};
  if (!state) state = {};
  if (typeof text === "undefined") text = "";
  if (!state.unlockedNPCs) state.unlockedNPCs = {};
  if (!state.region) state.region = "Kanto";
  if (!state.towns) state.towns = {};
  if (!state.reputation) state.reputation = {};
  if (!state.biomes) state.biomes = {};
  if (!state.pokemonData) state.pokemonData = {};
  if (!state.legends) state.legends = {};
  if (!state.pokemonBond) state.pokemonBond = {};
  if (!state.timeOfDay) state.timeOfDay = "day";
  if (!state.location) state.location = "Forest";
  if (!state.pokemonMoves) state.pokemonMoves = {};
  if (!state.flags) state.flags = {};
  if (!state.trainerLog) state.trainerLog = [];
  if (!state.npcMemory) state.npcMemory = {};
  if (!state.dreamFlags) state.dreamFlags = {};
  if (!state.inventory) state.inventory = {};
  if (!state.usedItems) state.usedItems = {};
  if (!state.keyItems || typeof state.keyItems.add !== "function") {
    state.keyItems = new Set(Array.isArray(state.keyItems) ? state.keyItems : []);
  }
  if (!state.pokemonItems) state.pokemonItems = {};
  if (!state.lastDreamDay) state.lastDreamDay = 0;
  if (!state.reputationLog) state.reputationLog = {};
  if (!state.pokedex) state.pokedex = {
    entries: {},
    version: "Kanto-Dex",
    unlockedEvolutions: new Set(),
    totalCaught: 0
  };
  if (!state.pokedex.unlockedEvolutions || typeof state.pokedex.unlockedEvolutions.add !== "function") {
    state.pokedex.unlockedEvolutions = new Set(Object.keys(state.pokedex.unlockedEvolutions || {}));
  }
  if (!state.globalEvents) {
    state.globalEvents = {
      "Riftwake": false,
      "Weather Collapse": false,
      "Ultra Anomalies": false,
      "Dreamers' Curse": false,
      "Shadow Uprising": false,
      "Plague of Fog": false,
      "Champion's Challenge": false,
      "Dimensional Breach": false,
      "Wild Swarm": false,
    };
  }
  
  // === PLAYER INITIALIZER: Paste this just before main keyword handler logic ===
if (!state.hasInitialized) {
  // Helper: Add item(s) to inventory
  function addItem(item, qty) {
    if (!state.inventory[item]) state.inventory[item] = 0;
    state.inventory[item] += qty;
  }
  // Helper: Add starter PokÃ©mon
  function addPokemon(name, starterType = "standard") {
    if (!state.pokemonData[name]) state.pokemonData[name] = {};
    state.pokemonData[name].starterType = starterType;
    // You can expand here for base stats, level, moves, etc.
  }
  // Helper: Capitalize
  function cap(s) { return s ? s.charAt(0).toUpperCase() + s.slice(1) : ""; }

  const occupation = (state.occupation || "Rookie Trainer").toLowerCase();
  let startMsg = "";

  switch (occupation) {
   case "rookie trainer":
  state.location = state.location || "Starter Town";
  startMsg = "You begin your journey as a Rookie Trainer, ready to receive your first PokÃ©mon from the Professor!";
  break;


    case "researcher":
      addItem("PokÃ© Ball", 10);
      addItem("Potion", 1);
      addItem("Escape Rope", 1);
      addPokemon("lab partner", "research");
      state.location = state.location || "PokÃ©mon Lab";
      startMsg = "You start your adventure as a PokÃ©mon Researcher, ready to unlock the mysteries of the world.";
      break;

    case "ace trainer":
      addItem("PokÃ© Ball", 5);
      addItem("Super Potion", 1);
      addPokemon("ace mon", "ace");
      state.location = state.location || "Battle Arena";
      startMsg = "You hit the scene as an Ace Trainer, already skilled in the art of battle.";
      break;

    case "breeder":
      addItem("PokÃ© Ball", 3);
      addItem("Potion", 2);
      addPokemon("egg1", "egg");
      addPokemon("egg2", "egg");
      state.location = state.location || "PokÃ©mon Day Care";
      startMsg = "You begin your path as a PokÃ©mon Breeder, surrounded by eggs and dreams.";
      break;

    case "ranger":
      addItem("PokÃ© Ball", 5);
      addItem("Potion", 2);
      addItem("Antidote", 2);
      addItem("Repel", 1);
      addPokemon("partner", "ranger");
      state.location = state.location || "Forest Outpost";
      startMsg = "You are a PokÃ©mon Ranger, sworn to protect the wild and maintain the balance of nature.";
      break;

    case "villain":
    case "villain/team member":
      addItem("PokÃ© Ball", 4);
      addItem("Potion", 1);
      addItem("Smoke Ball", 1);
      addPokemon("team mon", "villain");
      state.location = state.location || "Secret Base";
      startMsg = "You work in the shadows as a member of an infamous organization.";
      break;

    case "gym leaderâ€™s kid":
    case "gym leader's kid":
      addItem("PokÃ© Ball", 6);
      addItem("Potion", 2);
      addPokemon("gift mon", "gift");
      addItem("Gym Badge", 1);
      state.location = state.location || "Gym";
      startMsg = "You grew up surrounded by PokÃ©mon battlesâ€”the Gym is your second home.";
      break;

    case "gym leader":
      addItem("PokÃ© Ball", 10);
      addItem("Super Potion", 3);
      addPokemon("signature mon", "signature");
      addItem("Gym Badge", 8);
      state.location = state.location || "Your Gym";
      startMsg = "You are a Gym Leader, master of your domain.";
      break;

    case "police officer":
      addItem("PokÃ© Ball", 6);
      addItem("Potion", 2);
      addItem("Full Heal", 1);
      addItem("Handcuffs", 1);
      addPokemon("police partner", "partner");
      state.location = state.location || "Police Station";
      startMsg = "You serve as a Police Officer, keeping the region safe from crime.";
      break;

    case "pokÃ©mon doctor":
    case "pokÃ©mon nurse":
    case "pokemon doctor":
    case "pokemon nurse":
      addItem("PokÃ© Ball", 3);
      addItem("Potion", 4);
      addItem("Full Heal", 2);
      addPokemon("healer partner", "healer");
      state.location = state.location || "PokÃ©mon Center";
      startMsg = "You care for the sick and wounded as a PokÃ©mon Doctor or Nurse.";
      break;

    default:
      addItem("PokÃ© Ball", 3);
      addPokemon("companion", "misc");
      state.location = state.location || "Town";
      startMsg = "You begin your unique journey in the world of PokÃ©mon.";
  }

  // Initial money if not already set
  if (typeof state.money === "undefined") state.money = 3000;

  // Only print at very first setup (to avoid spamming reboots)
  output.push(startMsg);
  state.hasInitialized = true;
}


  if (!state.championLeagues || typeof state.championLeagues.add !== "function") state.championLeagues = new Set();

  // --- CHAMPION LEAGUE TRACKING ---
  const leagueWinMatch = text.match(/became champion(?: of)? ([A-Za-z\- ]+)(?: league)?/i);
  if (leagueWinMatch) {
    const leagueName = leagueWinMatch[1].trim();
    if (!state.championLeagues.has(leagueName)) {
      state.championLeagues.add(leagueName);
      state.timesChampion = (state.timesChampion || 0) + 1;
      output.push(`ðŸ… You are now registered as Champion of the ${leagueName} League!`);
    } else {
      output.push(`You're already the registered Champion of the ${leagueName} League.`);
    }
  }

 // --- PokÃ© Mart Inventory & Prices (Gen 9: Scarlet/Violet Standard) ---
const pokeMartInventory = [
  // Balls
  "PokÃ© Ball", "Great Ball", "Ultra Ball", "Heal Ball", "Net Ball", "Nest Ball",
  "Quick Ball", "Dusk Ball", "Timer Ball", "Luxury Ball", "Premier Ball", "Repeat Ball",
  // Medicine
  "Potion", "Super Potion", "Hyper Potion", "Max Potion", "Full Restore", "Revive", "Max Revive",
  "Antidote", "Paralyze Heal", "Burn Heal", "Ice Heal", "Awakening", "Full Heal",
  // Utilities
  "Escape Rope", "Repel", "Super Repel", "Max Repel",
  // Battle Items (select examples)
  "X Attack", "X Defense", "X Sp. Atk", "X Sp. Def", "X Speed", "X Accuracy", "Guard Spec.",
  // Held Items (select examples)
  "Soothe Bell", "Eviolite", "Everstone"
];

// Gen 9 PokÃ© Mart Prices (PokÃ©mon Scarlet/Violet standard)
const pokeMartPrices = {
  // Balls
  "PokÃ© Ball": 200, "Great Ball": 600, "Ultra Ball": 800, "Heal Ball": 300,
  "Net Ball": 1000, "Nest Ball": 1000, "Quick Ball": 1000, "Dusk Ball": 1000,
  "Timer Ball": 1000, "Luxury Ball": 3000, "Premier Ball": 200, "Repeat Ball": 1000,
  // Medicine
  "Potion": 200, "Super Potion": 700, "Hyper Potion": 1200, "Max Potion": 2500, "Full Restore": 3000,
  "Revive": 2000, "Max Revive": 4000,
  "Antidote": 200, "Paralyze Heal": 200, "Burn Heal": 200, "Ice Heal": 200,
  "Awakening": 200, "Full Heal": 400,
  // Utilities
  "Escape Rope": 1000, "Repel": 400, "Super Repel": 700, "Max Repel": 900,
  // Battle Items
  "X Attack": 1000, "X Defense": 1000, "X Sp. Atk": 1000, "X Sp. Def": 1000, "X Speed": 1000, "X Accuracy": 1000, "Guard Spec.": 1500,
  // Held Items (select examples)
  "Soothe Bell": 1000, "Eviolite": 3000, "Everstone": 3000
};


  if (!state.money) state.money = 3000;

  // --- KEYWORD HANDLERS ---
  const keywordHandlers = [
   
   {
  keywords: [
    /receive starter/i,
    /get my starter/i,
    /choose my starter/i,
    /pick my starter/i,
    /receive (my )?(first )?pok[Ã©e]mon/i,
    /get (my )?(first )?pok[Ã©e]mon/i,
    /receive pok[Ã©e]dex/i,
    /get my pok[Ã©e]dex/i,
    /take pok[Ã©e]dex/i,
    /collect my starter/i,
    /begin (my )?journey/i,
    /start (my )?adventure/i,
    /meet the professor/i,
    /accept my starter/i,
    /take my pok[Ã©e] balls/i
  ],
  action: () => {
    if (!state.hasReceivedStarter) {
      addPokemon("starter", "starter");
      addItem("PokÃ© Ball", 5);
      addItem("Potion", 2);
      state.hasReceivedStarter = true;
      output.push("ðŸŽ‰ You received your starter PokÃ©mon, a PokÃ©dex, 5 PokÃ© Balls, and 2 Potions from the Professor!");
    } else {
      output.push("You've already received your starter and supplies.");
    }
  }
},
  

    // Champion's Challenge
    {
      keywords: [/champion['â€™]?s challenge/i, /start tournament/i, /world tournament/i],
      action: () => {
        const championCount = state.championLeagues.size;
        if (championCount >= 3) {
          output.push("ðŸ† The Champion's Challenge is announced across the world!");
          state.globalEvents["Champion's Challenge"] = true;
        } else {
          output.push("You must become Champion in at least 3 different leagues before you can start the Champion's Challenge!");
        }
      }
    },
    // Type assignment
    {
      keywords: [
        /assign types? ([a-z]+)(?: and |\/)?([a-z]+)? to ([a-z0-9\- ]+)/i,
        /set ([a-z0-9\- ]+)'?s type(?:s)? to ([a-z]+)(?: and |\/)?([a-z]+)?/i,
        /make ([a-z0-9\- ]+) a[n]? ([a-z]+)(?:\/| and )?([a-z]+)? type/i
      ],
      action: () => {
        let typeMatch =
          text.match(/assign types? ([a-z]+)(?: and |\/)?([a-z]+)? to ([a-z0-9\- ]+)/i) ||
          text.match(/set ([a-z0-9\- ]+)'?s type(?:s)? to ([a-z]+)(?: and |\/)?([a-z]+)?/i) ||
          text.match(/make ([a-z0-9\- ]+) a[n]? ([a-z]+)(?:\/| and )?([a-z]+)? type/i);

        if (typeMatch) {
          let pokeName, type1, type2;
          if (typeMatch[3]) {
            type1 = capitalize(typeMatch[1]);
            type2 = typeMatch[2] ? capitalize(typeMatch[2]) : null;
            pokeName = typeMatch[3].trim();
          } else {
            pokeName = typeMatch[1].trim();
            type1 = capitalize(typeMatch[2]);
            type2 = typeMatch[3] ? capitalize(typeMatch[3]) : null;
          }
          if (!state.pokemonData[pokeName]) state.pokemonData[pokeName] = {};
          state.pokemonData[pokeName].type = type2 ? [type1, type2] : [type1];
          output.push(`${pokeName} is now a ${type2 ? type1 + "/" + type2 : type1} type!`);
        }
      }
    },
    // Mega Evolution, Dynamax, Gigantamax, Z-Move, Riftwake, etc. (Stub handlers!)
    // Add your own function implementations!
    {
      keywords: [/mega evolve/i, /use mega stone/i],
      action: () => output.push("âœ¨ Mega Evolution logic here! (You must add extractPokemonName and canMegaEvolve functions)")
    },
    {
      keywords: [/dynamax/i, /go dynamax/i, /use dynamax band/i],
      action: () => output.push("ðŸ’¥ Dynamax logic here! (You must add extractPokemonName and canDynamax functions)")
    },
    {
      keywords: [/gigantamax/i, /g-max/i, /gigantamax form/i],
      action: () => output.push("ðŸŒªï¸ Gigantamax logic here! (You must add extractPokemonName and canGigantamax functions)")
    },
    {
      keywords: [/z-move/i, /use z-move/i, /activate z power/i],
      action: () => output.push("âš¡ Z-Move logic here! (You must add extractPokemonName, getMoveType, exclusiveZMoves, zCrystalTypes, etc.)")
    },
    {
      keywords: [/riftwake/i, /activate riftwake/i, /summon rift/i],
      action: () => {
        output.push("ðŸŒŒ The world trembles as the Riftwake event is triggered!");
        state.globalEvents["Riftwake"] = true;
      }
    }
  ];

// List of keywords that should trigger registration
const registrationKeywords = [
  "register", "registration", "discovered", "new evolution", "new form",
  "regional form", "paradox", "variant", "documented", "not in pokÃ©dex",
  "never-before-seen", "first sighting"
];

// Monitors recent player/AI messages for registration keywords
function checkForRegistrationTrigger(message, pokemon, previousForm, region) {
  const lowered = message.toLowerCase();
  for (let kw of registrationKeywords) {
    if (lowered.includes(kw)) {
      registerNewEvolution(pokemon, previousForm, region);
      break;
    }
  }
}

  // --- Add PokÃ© Mart handlers at the end ---
  keywordHandlers.push(
    {
      keywords: [/visit pok[eÃ©] ?mart/i, /pok[eÃ©] ?mart/i, /open mart/i, /shop/i],
      action: () => {
        output.push("ðŸ›’ Welcome to the PokÃ© Mart! Here's what's in stock:");
        pokeMartInventory.forEach(item => {
          output.push(`- ${item} (${pokeMartPrices[item]}â‚½)`);
        });
        output.push(`You have ${state.money}â‚½.`);
      }
    },
    {
      keywords: [/buy (\d+)? ?([a-zÃ©' ]+)/i],
      action: () => {
        let buyMatch = text.match(/buy (\d+)? ?([a-zÃ©' ]+)/i);
        if (!buyMatch) return;
        let qty = buyMatch[1] ? parseInt(buyMatch[1], 10) : 1;
        let itemName = buyMatch[2].trim();
        let canonical = pokeMartInventory.find(it =>
          it.toLowerCase() === itemName.toLowerCase() ||
          it.toLowerCase().includes(itemName.toLowerCase())
        );
        if (!canonical) {
          output.push(`Sorry, we don't sell "${itemName}".`);
          return;
        }
        let price = pokeMartPrices[canonical] * qty;
        if (state.money >= price) {
          state.money -= price;
          if (!state.inventory[canonical]) state.inventory[canonical] = 0;
          state.inventory[canonical] += qty;
          output.push(`You bought ${qty} ${canonical}${qty > 1 ? "s" : ""} for ${price}â‚½. You have ${state.money}â‚½ left.`);
        } else {
          output.push(`You don't have enough money! You need ${price}â‚½.`);
        }
      }
    },
    {
      keywords: [/bag/i, /inventory/i, /check bag/i, /my items/i],
      action: () => {
        let inv = state.inventory;
        let bag = Object.keys(inv).filter(item => inv[item] > 0);
        if (!bag.length) {
          output.push("Your bag is empty!");
        } else {
          output.push("ðŸ‘œ In your bag:");
          bag.forEach(item => {
            output.push(`- ${item} x${inv[item]}`);
          });
        }
        output.push(`You have ${state.money}â‚½.`);
      }
    }
  );

  // --- Run all handlers ---
  for (let handler of keywordHandlers) {
    if (handler.keywords.some(rx => rx.test(text))) {
      handler.action();
    }
  }


