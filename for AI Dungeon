// --- POKÃ‰DEX REGISTRATION ---
const pokedex = {}; // Stores registration info

function registerToDex(pokemon) {
  if (!pokedex[pokemon.species]) {
    pokedex[pokemon.species] = {
      name: pokemon.species,
      forms: new Set(),
      evolutions: new Set(),
      firstDiscoveredBy: pokemon.owner || "Unknown"
      // Add other info you want to log
    };
  }
  pokedex[pokemon.species].forms.add(pokemon.form || "Base");
  pokedex[pokemon.species].evolutions.add(pokemon.evolvedFrom || "Starter");
  // ...expand as needed
}

// --- EXP MODIFIERS ---
function applyExpModifiers(baseExp, {expShare=false, luckyEgg=false, traded=false, affection=false} = {}) {
  let exp = baseExp;
  if (expShare) exp *= 0.5;         // Example, adjust as needed
  if (luckyEgg) exp *= 1.5;
  if (traded) exp *= 1.5;
  if (affection) exp *= 1.2;        // e.g., high friendship/affection
  return Math.floor(exp);
}

// --- GET EXP NEEDED FOR NEXT LEVEL ---
function getExpToNextLevel(pokemon) {
  const curve = expCurves[pokemon.expCurve || 'MediumFast'];
  return curve(pokemon.level + 1) - curve(pokemon.level);
}

// --- LEVEL UP CHECK, MOVE LEARNING, DEX UPDATE ---
function levelUpCheck(pokemon) {
  let curve = expCurves[pokemon.expCurve || 'MediumFast'];
  while (pokemon.exp >= curve(pokemon.level + 1)) {
    pokemon.level++;
    learnNewMoves(pokemon);
    checkEvolutionByObject(pokemon); // <-- FIXED HERE!
    registerToDex(pokemon); // Update PokÃ©dex on level up
    // Narrative event handler could be called here too
  }
}

// --- LEARN NEW MOVES AT LEVEL-UP ---
function learnNewMoves(pokemon) {
  const moves = moveSets[pokemon.species] || [];
  moves.forEach(entry => {
    if (entry.level === pokemon.level && !(pokemon.moves || []).includes(entry.move)) {
      if (!pokemon.moves) pokemon.moves = [];
      if (pokemon.moves.length < 4) {
        pokemon.moves.push(entry.move);
        // You could narrate: `${pokemon.name} learned ${entry.move}!`
      } else {
        // Handle move replacement logic
        // For now, just ignore if moveset full, or implement UI prompt as needed
      }
    }
  });
}

// --- EVOLUTION CHECK (Placeholder, expand with rules as needed) ---
function checkEvolutionByObject(pokemon) {
  // Example: Youâ€™ll want to expand this logic for all your species/trigger types!
  // e.g., if (pokemon.species === "Oshawott" && pokemon.level >= 17) { ... }
  // This is a placeholderâ€”expand for all your evolution rules!
}


// --- GRANT EXP TO POKEMON (after battle, catching, etc.) ---
function grantExp(pokemon, baseExp, modifiers) {
  const exp = applyExpModifiers(baseExp, modifiers);
  pokemon.exp = (pokemon.exp || 0) + exp;
  levelUpCheck(pokemon);
}

// --- CALCULATE EXP GAIN (Gen 9 logic, can expand further) ---
function calculateExpGain(foeLevel, isTrainerBattle=false) {
  let baseExp = Math.floor((foeLevel * foeLevel * foeLevel) / 7);
  if (isTrainerBattle) baseExp = Math.floor(baseExp * 1.5);
  return baseExp;
}

